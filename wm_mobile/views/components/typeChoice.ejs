<div class="card card-style mt-6 mb-3">
    <div class="content my-0">
        <div class="list-group list-custom-small">
            <a href='#' data-menu="menu-category-switch">
                <i class="fa fa-calendar color-blue-dark"></i>
                <span><%= types.find(c=>c.value===selectedType).label %></span>
                <i class="fa fa-angle-left"></i></a>
        </div>
    </div>
</div>
<div id="menu-category-switch" class="menu menu-box-bottom menu-box-detached rounded-m" data-menu-height="305" data-menu-effect="menu-over">
    <div class="list-group list-custom-small ps-1 pe-3">
        <% types.forEach(d=>{ %>
            <a href="/events?type=<%=d.value%>">
                <% if(selectedType===d.value){ %>
                    <i class="font-18 fa fa-circle color-red-light"></i>
                    <% }else{ %>
                        <i class="font-18 fa fa-circle color-teal-light"></i>
                        <% } %>                                        
                <span class="font-13">
                    <%= d.label %>
                </span>
            </a>
            <% }) %>
    </div>
</div>